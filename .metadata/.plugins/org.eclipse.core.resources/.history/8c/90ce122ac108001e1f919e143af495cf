	<script>
		//input(type=file) ÌååÏùºÏù¥ Ï∂îÍ∞ÄÎêòÏóàÎã§! -> Ïù¥Î≤§Ìä∏Í∞Ä Î∞úÏÉùÎêòÏóàÎã§!
		let photo = document.getElementById("photo")
		photo.addEventListener("change", e => {
			readImage(e.target)
		})
		
		function readImage(input){
			//Input ÌÉúÍ∑∏Ïóê ÌååÏùºÏù¥ ÏûàÎäî Í≤ΩÏö∞
			if(input.files && input.files[0]){
				//FileReader ÏÉùÏÑ±
				const reader = new FileReader()
				reader.onload = e=>{//FileReaderÍ∞Ä ÏÉùÏÑ±ÎêòÎ©¥
					//previewImage Í≥µÍ∞ÑÏóê Ï∂îÍ∞ÄÌïú Ïù¥ÎØ∏ÏßÄ Ï∂úÎ†•
					const previewImage = document.getElementById("preview-image")
					previewImage.src = "data:image/jpeg;base64,"
				}
			}
		}
	</script>
