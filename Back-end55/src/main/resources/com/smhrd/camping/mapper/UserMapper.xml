<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="com.smhrd.camping.mapper.UserMapper">
  	<insert id="login" parameterType="com.smhrd.camping.domain.User">
  		insert into TB_USER(USER_EMAIL,USER_PW)
  		values(#{user_email},#{user_pw})
  	</insert>
  
  </mapper>